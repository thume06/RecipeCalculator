<AnchorPane onMouseEntered="#ScreenLoaded" prefHeight="680.0" prefWidth="534.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.SavedRecipes">
   <children>
      <ImageView fx:id="background" fitHeight="680.0" fitWidth="534.0" onMouseEntered="#ScreenLoaded" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../assets/images/background.jpg" />
         </image>
      </ImageView>
      <ListView fx:id="recipeList" layoutX="58.0" layoutY="73.0" prefHeight="517.0" prefWidth="423.0" />
      <Button layoutX="58.0" layoutY="613.0" mnemonicParsing="false" onAction="#LoadSelectedRecipe" text="Load Selected Recipe">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="329.0" layoutY="613.0" mnemonicParsing="false" onAction="#ReturnToCalculator" prefHeight="30.0" prefWidth="152.0" text="Return to Calculator">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
